{
	"name": "@rtp2httpd/mpegts.js",
	"version": "3.1.0",
	"type": "module",
	"description": "HTML5 MPEG2-TS Stream Player (rtp2httpd fork)",
	"author": "Stackie Jia <jsq2627@gmail.com>",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/stackia/mpegts.js"
	},
	"keywords": [
		"html5",
		"mpegts",
		"m2ts",
		"mse",
		"arib",
		"dvb"
	],
	"main": "./dist/mpegts.esm.js",
	"module": "./dist/mpegts.esm.js",
	"types": "./dist/mpegts.d.ts",
	"exports": {
		".": {
			"import": "./dist/mpegts.esm.js",
			"types": "./dist/mpegts.d.ts"
		},
		"./wasm/mp2_decoder.wasm": "./wasm/minimp3/mp2_decoder.wasm"
	},
	"files": [
		"dist",
		"src",
		"wasm/minimp3/mp2_decoder.wasm"
	],
	"scripts": {
		"build": "vite build --mode=production",
		"build:debug": "vite build --mode=development",
		"dev": "vite",
		"preview": "vite preview",
		"type-check": "tsc --noEmit",
		"lint": "biome check .",
		"format": "biome check --fix ."
	},
	"dependencies": {},
	"devDependencies": {
		"@biomejs/biome": "^2.4.4",
		"@types/express-serve-static-core": "5.1.1",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"vite-bundle-analyzer": "^1.3.6",
		"vite-plugin-dts": "^4.5.4"
	},
	"packageManager": "pnpm@10.25.0",
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
